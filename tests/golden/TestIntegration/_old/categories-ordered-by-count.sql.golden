 order by count, c.name
    group by c.name, project {name, count(1) as count}
        join using nested loop
            join using nested loop
                project {film_id, title, description, release_year, language_id, original_language_id, rental_duration, rental_rate, length, replacement_cost, rating, last_update} as f.{...}
                    table scan of film
